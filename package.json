    "axios": "^0.19.2",
    "laravel-mix": "^5.0.1",
    "lodash.throttle": "^4.1.1",
    "luxon": "^1.22.2",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "svg-spritemap-webpack-plugin": "^3.5.1",
    "svg4everybody": "^2.1.9",
    "tailwindcss": "^1.2.0",
    "vue-template-compiler": "^2.6.11"
  }
